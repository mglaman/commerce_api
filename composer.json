{
    "name": "drupal/commerce_api",
    "type": "drupal-module",
    "description": "Provides an HTTP interface to interact with Drupal Commerce.",
    "keywords": [
        "Drupal"
    ],
    "license": "GPL-2.0+",
    "homepage": "https://www.drupal.org/project/commerce_api",
    "minimum-stability": "dev",
    "support": {
        "issues": "https://www.drupal.org/project/issues/commerce_api",
        "source": "http://cgit.drupalcode.org/commerce_api"
    },
    "require": {
        "drupal/commerce": "^2.15",
        "drupal/commerce_shipping": "2.x-dev",
        "drupal/jsonapi_hypermedia": "^1.0",
        "drupal/jsonapi_resources": "^1.0@beta",
        "drupal/core": "^8.8@alpha"
    },
    "extra": {
        "patches": {
            "drupal/commerce": {
                "Add an availability checker which checks if the entity is published or not": "https://www.drupal.org/files/issues/2020-01-17/3088598-3.patch",
                "Provide a constraint on the Coupons field to verify referenced coupons are available": "https://www.drupal.org/files/issues/2020-01-17/3041856-22.patch",
                "Add a constraint to purchased_entity which checks the availability manager": "https://www.drupal.org/files/issues/2020-01-17/3088597-7.patch",
                "Introduce a checkout init event": "https://www.drupal.org/files/issues/2020-01-17/3104564-9.patch"
            }
        }
    }
}
